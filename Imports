# pyplot from matplot lib
import matplotlib.pyplot as plt
https://matplotlib.org/stable/api/index.html

# pandas for dataframe manipulation
import pandas as pd
https://pandas.pydata.org/docs/reference/index.html

# seaborn for data visualization
import seaborn as sns
https://seaborn.pydata.org/api.html

# pretty print to clean up printing of dictionaries
import pprint
pp = pprint.PrettyPrinter()
https://docs.python.org/3/library/pprint.html

# import the IESVE api for the energy modeling software
import iesve
https://help.iesve.com/ve2021/6_ve_python_api.htm?ms=AhAAAAAAAAAAAAAAAgAAAAAAAAAAAAAAAAAAAgAAAAAAAAAAIAAAAAAAAAAAAAAAAEAEAQgC&st=MA%3D%3D&sct=MA%3D%3D&mw=MjQw
